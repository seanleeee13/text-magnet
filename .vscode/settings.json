{
    "files.autoSave": "afterDelay",
    "workbench.colorTheme": "Default High Contrast Light",
    "workbench.iconTheme": "material-icon-theme",
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "python.createEnvironment.trigger": "off",
    "material-icon-theme.folders.associations": {
        "cpp": "src",
        "compile": "dist"
    },
    "files.associations": {
        "*.cpp": "cpp",
        "iostream": "cpp",
        "xstring": "cpp",
        "vector": "cpp",
        "atomic": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "iterator": "cpp",
        "limits": "cpp",
        "memory": "cpp",
        "new": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "utility": "cpp",
        "xiosbase": "cpp",
        "xlocale": "cpp",
        "xlocnum": "cpp",
        "xmemory": "cpp",
        "xstddef": "cpp",
        "xtr1common": "cpp",
        "xutility": "cpp"
    },
    "code-runner.ignoreSelection": true
}